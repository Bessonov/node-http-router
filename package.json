{
	"name": "@bessonovs/node-http-router",
	"description": "Extensible http router for node and micro",
	"keywords": [
		"router",
		"routing",
		"route",
		"micro",
		"match",
		"request",
		"api",
		"rest",
		"typescript",
		"javascript",
		"nodejs",
		"http",
		"server"
	],
	"version": "0.0.4",
	"author": "Anton Bessonov",
	"license": "MIT",
	"repository": "bessonov/node-http-router",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"test": "jest --coverage",
		"test:watch": "jest --coverage --watch",
		"lint": "eslint . --ext .ts,.js",
		"build": "tsc",
		"example-node-start": "tsc && node dist/examples/node.js",
		"example-micro-start": "tsc && node dist/examples/micro.js",
		"precommit": "$_ run test && $_ run lint && $_ run build"
	},
	"dependencies": {
		"fast-url-parser": "1.1.3",
		"ts-toolbelt": "6.7.7"
	},
	"devDependencies": {
		"@bessonovs/eslint-config": "0.0.6",
		"@types/express": "4.17.6",
		"@types/jest": "25.2.2",
		"@types/node": "14.0.1",
		"@typescript-eslint/eslint-plugin": "2.33.0",
		"@typescript-eslint/parser": "2.33.0",
		"eslint": "7.0.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-plugin-import": "2.20.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.20.0",
		"jest": "26.0.1",
		"micro": "9.3.5-canary.3",
		"node-mocks-http": "1.8.1",
		"path-to-regexp": "6.1.0",
		"ts-jest": "26.0.0",
		"typescript": "3.9.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
